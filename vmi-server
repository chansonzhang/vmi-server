# @File    : vmi-server
# This script will be add to /etc/init.d/
# chkconfig: 345 99 01
# description: vmi-server
start() {
        echo "Starting vmi-server"
        cd /root/vmi-server
        ./startup.sh
}
stop() {
         echo "Starting vmi-server"
        cd /root/vmi-server
        ./shutdown.sh
}